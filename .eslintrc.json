{
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "plugins": [
      "import",
      "react",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  }
  